{
  "name": "kee-note",
  "appName": "KeeNote",
  "version": "4.0.5",
  "description": "A encrypted note editor, powered by KeePass.",
  "license": "GPLV3",
  "author": "canwdev",
  "main": "dist-src/main.js",
  "scripts": {
    "build-src": "tsc",
    "watch-src": "tsc -w",
    "dev": "cross-env NODE_ENV=development electron ./dist-src/main.js",
