{
  "name": "kee-note",
  "appName": "KeeNote",
  "version": "4.3.0",
  "description": "An encrypted note editor powered by KeePass.",
  "license": "GPLV3",
  "author": "canwdev",
  "main": "dist-src/main.js",
  "scripts": {
    "build:electron-src": "cross-env rimraf ./dist-src/* && npm run copy-files && tsc",
    "watch:electron-src": "cross-env npm run copy-files && tsc -w",
    "dev:electron": "cross-env NODE_ENV=development electron ./dist-src/main.js",
    "build:electron": "npm run build:electron-src && electron-builder --dir",
    "start:electron": "electron ./dist-src/main.js",
    "build:nest": "npm run build:electron-src -- -p tsconfig-nest-only.json",
    "watch:nest": "npm run watch:electron-src -- -p tsconfig-nest-only.json",
    "dev:nest": "cross-env NODE_ENV=development nodemon ./dist-src/nest/index.js",
    "start:nest": "node ./dist-src/nest/index.js",
    "copy-files": "copyfiles -u 1 src/common/keepass/support/argon2/argon2-asm.min.js dist-src",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^9.0.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "copyfiles": "^2.4.1",
    "electron": "15.5.7",
    "electron-builder": "^25.1.8",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@electron/remote": "^2.1.1",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/serve-static": "^3.0.1",
    "@xmldom/xmldom": "^0.7.4",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "electron-store": "^8.1.0",
    "fflate": "^0.7.1",
    "file-type": "^16.5.3",
    "fs-extra": "^10.1.0",
    "kdbxweb": "^2.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0"
  },
  "_build_comment": "https://github.com/electron-userland/electron-builder/issues/3573#issuecomment-450145085",
  "build": {
    "files": [
      "!node_modules/electron/**/*"
    ]
  }
}
